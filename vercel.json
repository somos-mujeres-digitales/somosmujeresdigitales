{
  "rewrites": [
    {
      "source": "/",
      "has": [
        { "type": "host", "value": "retosteam.com" },
        { "type": "host", "value": "www.retosteam.com" }
      ],
      "destination": "/retosteamcusco"
    },
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "retosteam.com" },
        { "type": "host", "value": "www.retosteam.com" }
      ],
      "destination": "/retosteamcusco/$1"
    }
  ]
}
